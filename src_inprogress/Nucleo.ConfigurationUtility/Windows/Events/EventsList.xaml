<UserControl x:Class="Nucleo.ConfigurationUtility.Windows.Events.EventsList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:idp="clr-namespace:Infragistics.Windows.DataPresenter;assembly=Infragistics3.Wpf.DataPresenter.v7.2"
    Height="300" Width="300">
	<idp:XamDataPresenter Name="EventList">
		<idp:XamDataPresenter.FieldLayouts>
			<idp:FieldLayout>
				<idp:FieldLayout.Fields>
					<idp:Field Name="Name"/>
					<idp:Field Name="Location"/>
					<idp:Field Name="BeginDate" Label="Begin Date"/>
				</idp:FieldLayout.Fields>
			</idp:FieldLayout>
		</idp:XamDataPresenter.FieldLayouts>
		<idp:XamDataPresenter.FieldLayoutSettings>
			<idp:FieldLayoutSettings AutoGenerateFields="False"/>
		</idp:XamDataPresenter.FieldLayoutSettings>
	</idp:XamDataPresenter>
</UserControl>
